// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  username  String?
}

model Game {
  id           Int      @id @default(autoincrement())
  title        String
  timer        Int
  date         DateTime
}

model Succes {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  date        DateTime
}